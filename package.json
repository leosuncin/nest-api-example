    "@nestjs/testing": "^8.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.7.0",
