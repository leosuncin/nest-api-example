{
  "name": "nest-api-example",
  "version": "0.0.0-development",
  "private": true,
  "description": "",
  "license": "GPL-3.0",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "nest build",
    "fixtures": "fixtures --require dotenv/config -c ormconfig.js fixtures",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "NODE_OPTIONS='-r dotenv/config' nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --selectProjects 'Unit test'",
    "test:cov": "jest --selectProjects 'Unit test' --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --selectProjects 'E2E test'",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit --noErrorTruncation --pretty false --incremental false -p tsconfig.build.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/typeorm": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@faker-js/faker": "^6.1.2",
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jest-dev-server": "^5.0.0",
    "@types/node": "^17.0.25",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^42.0.0",
    "fast-check": "^2.24.0",
    "fluse": "^1.1.0",
    "fluse-plugin-typeorm": "^1.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-dev-server": "^6.0.3",
    "jest-mock-extended": "^2.0.5",
    "lint-staged": "^12.4.0",
    "node-mocks-http": "^1.11.0",
    "pactum": "^3.1.6",
    "pg-mem": "^2.3.5",
    "prettier": "^2.6.2",
    "prettier-plugin-package": "^1.3.0",
    "semantic-release": "^19.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "txtgen": "^3.0.1",
    "typeorm-fixtures-cli": "^1.11.1",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leosuncin/nest-api-example.git"
  }
}
