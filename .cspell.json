{
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    ".vscode",
    "megalinter",
    ".gitignore",
    "pnpm-lock.yaml",
    "report",
    "dist/**",
    "**/coverage/**"
  ],
  "language": "en,es",
  "noConfigSearch": true,
  "allowCompoundWords": true,
  "words": [
    "pnpm",
    "nestjs",
    "typeorm",
    "Typeorm",
    "fluse",
    "pactum",
    "oneof",
    "Suncin",
    "Contraseña",
    "usuario",
    "contraseña",
    "cace",
    "huskyrc",
    "venv",
    "icrawl",
    "haya",
    "busa",
    "cond",
    "slavcodev",
    "pgweb",
    "sosedoff",
    "isready",
    "stefanzweifel",
    "dowuble",
    "Posgre",
    "Kamil",
    "Myśliwiec"
  ],
  "dictionaries": [
    "en_US",
    "companies",
    "softwareTerms",
    "misc",
    "lorem-ipsum",
    "typescript",
    "node",
    "bash",
    "npm"
  ],
  "languageSettings": [
    { "languageId": "commit-msg", "ignoreRegExpList": ["/^#.*/gm"] }
  ],
  "version": "0.2"
}
